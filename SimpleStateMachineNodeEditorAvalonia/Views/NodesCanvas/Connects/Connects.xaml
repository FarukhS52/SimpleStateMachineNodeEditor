<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:Views="clr-namespace:SimpleStateMachineNodeEditorAvalonia.Views"
			 xmlns:ViewModels="clr-namespace:SimpleStateMachineNodeEditorAvalonia.ViewModels"
             x:Class="SimpleStateMachineNodeEditorAvalonia.Views.NodesCanvasElements.Connects" ClipToBounds="False">
	<ItemsControl  x:Name="ItemsControlConnects" ClipToBounds="False">
		<ItemsControl.ItemsPanel>
			<ItemsPanelTemplate>
				<Canvas ClipToBounds="False"/>
			</ItemsPanelTemplate>
		</ItemsControl.ItemsPanel>
		<ItemsControl.DataTemplates>
			<DataTemplate DataType="{x:Type ViewModels:ConnectViewModel}">
				<Views:Connect/>
			</DataTemplate>
		</ItemsControl.DataTemplates>
	</ItemsControl>
</UserControl>
