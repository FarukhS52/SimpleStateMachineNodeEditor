<Styles xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">   
    <Style Selector="Ellipse.EllipseLeftConnector">
        <Setter Property="Fill" Value="{DynamicResource ColorLeftConnectorEllipseFill}" />
		<Setter Property="Stroke" Value="{DynamicResource ColorLeftConnectorEllipseStroke}" />
    </Style>

	<Style Selector="Ellipse:pointerover.EllipseLeftConnector">
		<Setter Property="Fill" Value="{DynamicResource ColorLeftConnectorEllipseFillPointerOver}" />
		<Setter Property="Stroke" Value="{DynamicResource ColorLeftConnectorEllipseStrokePointerOver}" />
	</Style>

</Styles>