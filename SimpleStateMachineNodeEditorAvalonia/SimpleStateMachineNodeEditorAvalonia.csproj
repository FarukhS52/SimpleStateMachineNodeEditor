    <None Remove="Views\Node\Connectors\Connectors.xaml" />
    <AvaloniaXaml Include="Views\Node\Connectors\Connectors.xaml">
      <Generator>MSBuild:Compile</Generator>
    </AvaloniaXaml>
    <Folder Include="Behaviors\" />
<!--	<AvaloniaResource Include="Styles\**" />-->
