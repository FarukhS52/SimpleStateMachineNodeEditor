    <AvaloniaResource Remove="Views\Node\Connectors\Connectors.xaml" />
    <Compile Remove="Views\EmptyTextBox.xaml.cs" />
    <None Remove="Styles\Connector\LeftConnector\TextBox\TextBoxLeftConnectorDefault.xaml" />
    <None Remove="Styles\Connector\RightConnector\TextBox\TextBoxRightConnectorDefault.xaml" />
    <None Remove="Styles\Node\Header\Button\NodeHeaderButton.xaml" />
