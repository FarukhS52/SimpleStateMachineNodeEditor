<UserControl x:Class="SimpleStateMachineNodeEditor.View.ViewConnect"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SimpleStateMachineNodeEditor.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" IsHitTestVisible="False">
    <Grid>
        <Path x:Name="PathElement" StrokeDashCap="Round" StrokeLineJoin="Round" Stroke="{StaticResource ColorConnect}" StrokeEndLineCap="Round" StrokeStartLineCap="Round">
            <Path.Data>
                <PathGeometry  x:Name="PathGeometryElement">
                    <PathFigure x:Name="PathFigureElement"  IsClosed="False">
                        <BezierSegment  x:Name="BezierSegmentElement"/>
                    </PathFigure>
                </PathGeometry>
            </Path.Data>
        </Path>
    </Grid>
</UserControl>
